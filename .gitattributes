*.css   text eol=lf
*.html  text eol=lf
*.js    text eol=lf
*.json  text eol=lf
*.md    text eol=lf
*.pug   text eol=lf
*.scss  text eol=lf
*.svg   text eol=lf
*.ts    text eol=lf
*.vue   text eol=lf
*.yml   text eol=lf
.editorconfig   text eol=lf
.gitattributes  text eol=lf
.gitignore      text eol=lf
LICENSE         text eol=lf

# Don't diff or textually merge source maps
*.map   binary

