  "repository": {
    "type": "git",
    "url": "git+https://github.com/archco/wise-quotes-client.git"
  },
  "author": "archco",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/archco/wise-quotes-client/issues"
  },
  "homepage": "https://github.com/archco/wise-quotes-client#readme",
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "eslint": "^4.18.2",
    "jest": "^22.4.2",
    "mocha": "^5.0.4",
    "source-map-loader": "^0.2.3",
    "ts-jest": "^22.4.1",
    "ts-loader": "^4.0.1",
    "typescript": "^2.7.2",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.11",
    "webpack-merge": "^4.1.2",
    "webpack-notifier": "^1.6.0",
    "wise-quotes": "^0.3.0"
  },
  "jest": {
    "transform": {
